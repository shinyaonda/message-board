<div class="panel panel-primary">
 <div class="panel-heading">
   Messages
 </div>
 
 <div class="panel-body">
  <% @messages.each do |message| %>
   <span class="label label-info"><%= message.name %></span>
   <%= message.body %>
   (<%= link_to "edit" , edit_message_path(message) %>)
    <br>
   <% end %>
   </div>
 </div> 
 
